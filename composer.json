{
    "name": "guspio/monolog-datadog",
    "description": "Monolog Handler for Datadog Logs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gusp",
            "email": "contact@gusp.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "monolog/monolog": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0 | ~9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MonologDatadog\\": "src/Monolog"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonologDatadog\\": "tests/Monolog"
        }
    }
}